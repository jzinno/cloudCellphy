profiles {
    stub {
        stubRun = true
        process {
            {
                executor = 'local'
                cpus = 1
                memory = '1 GB'
                time = '1h'
            }
        }
        params{
            sample_id = "test"
            joint_vcf = "test/test.chr21.vcf.gz"
        }
    }
}

process {
    executor = 'slurm'
    errorStrategy = { task.exitStatus in [143,137,104,134,139] ? 'retry' : 'terminate' }
    maxRetries = 2
    maxErrors = '-1'
    cache = 'lenient'
}

executor {
  queueSize = 196
  submitRateLimit = '100/2min'
}


params {
    joint_vcf = "test/test.chr21.vcf.gz"
    n_tree_search = 10
    n_bootstrap_search = 10
    bs_trees_per_job = 10
    out = "output"
    sample_id = "test"
}
